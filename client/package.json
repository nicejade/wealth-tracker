{
  "name": "client",
  "description": "生财有迹（Wealth Tracker），客户端代码实现。",
  "version": "0.3.0",
  "author": "nicejade",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run routify && vite",
    "build": "vite build",
    "preview": "vite preview",
    "routify": "npx @roxi/routify build && cp ./src/components/404.svelte .routify/components/[...404].svelte",
    "deploy": "npm run routify && vite build",
    "gen-sitemap": "node scripts/gen-sitemap.js",
    "prettier": "prettier --plugin prettier-plugin-svelte --write \"src/**/**/*.{svelte,js,ts,jsx,tsx,css,scss,vue}\"",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@skeletonlabs/skeleton": "^1.12.0",
    "axios": "^1.3.3",
    "canvas-confetti": "^1.9.3",
    "dayjs": "^1.11.10",
    "flowbite": "^1.6.3",
    "flowbite-svelte": "^0.44.24",
    "mdsvex": "^0.10.6",
    "svelte": "^4.2.18",
    "svelte-i18n": "^4.0.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@roxi/routify": "^3.0.0-next.160",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
